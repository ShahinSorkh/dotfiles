{
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "discard-changes": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "require": {
        "d11wtq/boris": "@stable",
        "friendsofphp/php-cs-fixer": "@stable",
        "lctrs/psalm-psr-container-plugin": "@stable",
        "orklah/psalm-strict-visibility": "@stable",
        "phan/phan": "@stable",
        "php-standard-library/psalm-plugin": "@stable",
        "phpactor/phpactor": "@stable",
        "phpstan/phpstan": "@stable",
        "phpunit/phpunit": "@stable",
        "psalm/plugin-mockery": "@stable",
        "psalm/plugin-phpunit": "@stable",
        "psy/psysh": "@stable",
        "techlivezheng/phpctags": "@stable",
        "vimeo/psalm": "@stable"
    }
}
